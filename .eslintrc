{
  "extends": "bliss",
  "env": {
    "browser": true
  },
  "globals": {
    "THREE": true
  },
  "rules": {
    "fp/no-mutation": "off",
    "fp/no-rest-parameters": "off",
    "fp/no-this": "off",
    "fp/no-loops": "off",
    "fp/no-let": "off",
    "fp/no-throw": "off",
    "fp/no-class": "off"
  }
}
